{
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "ecmaFeatures": {
    "jsx": true
  },
  "rules": {
    "react/jsx-filename-extension": [
      1,
      { 
        "extensions": [".js", ".jsx"] 
      }
    ],
    "comma-dangle": [
      "error",
      {
        "functions": "ignore"
      }

    ],
    "no-debugger": 0
  },
  "extends": ["airbnb"]
}

